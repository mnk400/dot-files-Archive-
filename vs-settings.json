{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.configuration.checkProjectSettingsExclusions": false,
  "editor.fontFamily": "Victor Mono",
  "editor.fontSize": 15,
  "editor.fontLigatures": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.renderIndentGuides": false,
  "editor.hideCursorInOverviewRuler": true, 
  "editor.overviewRulerBorder": false,
  "update.mode": "manual",
  "git.enableSmartCommit": true,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.rulers": [
    79,
    130
  ],
  "editor.tokenColorCustomizations": {
      "textMateRules": [
        {
          "scope": [
            //following will be in italic
            "comment",
            "comment.block",
            "comment.block.documentation",
            "string.quoted.docstring.multi.python",
            "entity.name.type.class", //class names
            "keyword", //import, export, return…
            "constant", //String, Number, Boolean…, this, super
            "storage.modifier", //static keyword
            "storage.type.class.js", //class keyword
          ],
          "settings": { 
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            //following will be excluded from italics
            "invalid",
            "keyword.operator",
            "constant.numeric.css",
            "keyword.other.unit.px.css",
            "constant.numeric.decimal.js",
            "constant.numeric.json"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": [
            "comment",
          ],
          "settings": { 
            "foreground":"#787878",
          }
        },
      ]
    },
  "workbench.colorCustomizations": {
      "activityBar.background": "#141414",
      "editor.background": "#0a0a0a",
      "titleBar.activeBackground": "#141414",
      "sideBar.background": "#141414",
      "editorRuler.foreground": "#202020",
      "sideBarSectionHeader.background":"#141414",
      "editorGroupHeader.tabsBackground": "#0a0a0a",
      "tab.activeBackground": "#242424",
      "tab.inactiveBackground": "#0a0a0a",
      "tab.hoverBackground": "#101010", 
      "tab.border": "#0a0a0a",
      "editorBracketMatch.background": "#353535",
      "editorBracketMatch.border": "#353535",
      "statusBar.background": "#5a2f3c",
      "statusBar.foreground": "#d1c1c6",
      "badge.background": "#5a2f3c",
      "activityBarBadge.background": "#5a2f3c",
      "editor.selectionBackground": "#333333",
      "editor.lineHighlightBackground": "#202020",
      "scrollbar.shadow": "#00000000",
      "peekView.border": "#00000000",
      "peekViewResult.background":"#202020",
      "peekViewTitle.background": "#202020",
      "statusBarItem.remoteBackground": "#435748",
      "statusBarItem.remoteForeground": "#ccdbd0",
      "statusBar.noFolderBackground": "#40364e",
      "statusBar.debuggingBackground": "#77412e"
  },
  "breadcrumbs.enabled": true,
  "editor.renderWhitespace": "none",
  "customizeUI.font.regular": "Victor Mono",
  "customizeUI.activityBar": "bottom",
  "customizeUI.moveStatusbar": false,
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.autoBuild.interval": 500,
  "explorer.confirmDelete": false,
  "python.linting.flake8CategorySeverity.F": "Information",
  "[python]": {},
  "git.confirmSync": false,
  "python.dataScience.allowUnauthorizedRemoteConnection": true,
  "window.zoomLevel": 0,
  "python.testing.unittestEnabled": true,
  "editor.accessibilitySupport": "off",
  "kite.showWelcomeNotificationOnStartup": false,
  "python.pythonPath": "C:\\Python39\\python.exe",
  "python.dataScience.askForKernelRestart": false,
  "window.nativeTabs": true,
  "python.linting.flake8CategorySeverity.E": "Hint",
  "java.semanticHighlighting.enabled": true,
  "python.languageServer": "Pylance",
  "workbench.iconTheme": "file-icons",
  "latex-workshop.latex.clean.subfolder.enabled": true,
  "latex-workshop.message.update.show": false,
  "java.project.importHint": false,
  "git.autofetch": true,
  "terminal.integrated.rendererType": "dom",
  "python.linting.pylamaEnabled": true,
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "workbench.sideBar.location": "left",
  "terminal.integrated.gpuAcceleration": "off",
  "python.defaultInterpreterPath": "C:\\Python39\\python.exe",
  "editor.inlineSuggest.enabled": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "github.copilot.enable": {
    "*": false,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
}